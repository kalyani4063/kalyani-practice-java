// Online Java Compiler
// Use this editor to write, compile and run your Java code online
// program to find gcd 
class Main {
    
      static int gcd(int a , int b ){
 int max=0;
 int gcd=0;
 
 for(int i=1; i<=a; i++)
 {
     
     if(a%i==0&& b%i==0)
     {
         if(i>max)
         {
             gcd=i;
         }
     }
 }
 return gcd;
 }
    
    public static void main(String[] args) {
        

 

 int c=gcd(28,63);
 System.out.println(c);
 
    
}
      
    
}
